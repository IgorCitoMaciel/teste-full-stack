{
  "name": "server",
  "version": "1.0.0",
  "main": "main.ts",
  "repository": "https://github.com/Grupo-Hygea-Saude/test-full-stack",
  "author": "Diego Jimenes",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "docker-up": "docker-compose -f docker-compose.yml up --detach",
    "docker-down": "docker-compose stop",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "migration:run": "yarn typeorm migration:run -d src/main.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --coverage"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17"
  }
}
